[user]
    name = yamasy1549
    email = sanae@yamasy.info
[core]
    autocrlf = input
    editor = vim
    excludesfile = ~/.gitignore_global
[color]
    ui = true
[alias]
    co = checkout
    cb = checkout -b
    st = git-st
    sta = git-sta
    std = git-std
    cv = commit -v
    ca = commit --amend
    ic = commit --allow-empty -m 'Initial commit'
    rbm = rebase master
    mm = merge master
